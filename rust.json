{
  "id" : "rust",
  "version" : "0.0.1",
  "description": "Rust language runner for gauge",
  "run": {
    "windows": ["./bin/gauge_rs.exe", "--run"],
    "linux": ["./bin/gauge_rs", "--run"],
    "darwin": ["./bin/gauge_rs", "--run"]
  },
  "init": {
    "windows": ["./bin/gauge_rs.exe", "--init"],
    "linux": ["./bin/gauge_rs", "--init"],
    "darwin": ["./bin/gauge_rs", "--init"]
  },
  "gRPCSupport": true,
  "lib": "libs",
  "gaugeVersionSupport": {
    "minimum": "1.0.7",
    "maximum": ""
  }
}
